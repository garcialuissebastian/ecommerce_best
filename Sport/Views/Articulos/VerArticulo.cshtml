
@{
    ViewBag.Title = "VerArticulo";
    Layout = "~/Views/Master/_sport.cshtml";

    Bll.QueryString obQueryString = new Bll.QueryString();

    string urlanterior = "/Articulos/";
    string ParametroIzq = "";
