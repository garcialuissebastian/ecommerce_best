                                        @{

                                            string tieneStock = "no";

                                            foreach (var item in @articulo.Talles)
                                            {
                                                obQueryString = new Bll.QueryString();

                                                Bll.QueryString.FromCurrent();
                                                obQueryString.Add("vart", item.id_art);

                                                string ParametroCompra = Bll.Encriptor.DameInstancia().EncryptQueryString(obQueryString).ToString();

                                                string tallever = string.IsNullOrEmpty(item.ARG) ? item.talle : item.ARG;

                                                if (Convert.ToInt32(item.stock) > 0)
                                                {
                                                    tieneStock = "si";

                                                    if (item.id_art != articulo.Id)
                                                    {

                                                        <li    onmouseover="overTalle(this)" onmouseout="outTalle()" data-STK="@item.stock" data-UK="@item.UK" data-US="@item.US" data-CM="@item.CM" data-ARG="@item.ARG" disabled><a href="/Articulos/VerArticulo/@ParametroCompra"> @tallever</a></li>

                                                    }
