            var numberFormatInfo = new System.Globalization.NumberFormatInfo();
            numberFormatInfo.NumberDecimalSeparator = ".";
            numberFormatInfo.CurrencyPositivePattern = 0;

            Be.Best_articulos_ventaId articulo = (Be.Best_articulos_ventaId)ViewData["articulo"];
            Int32 tipo1 = 0;


            tipo1 = Convert.ToInt32(articulo.tipo_articulo_id);


            string cat = "";


            cat = articulo.clasificacion_id;


            Be.Best_tipos_articulos objTipo = null;

            List<Be.Best_Menu> menu = (List<Be.Best_Menu>)ViewData["menu"];

            if (tipo1 > 0)
            {
                foreach (var item in menu)
                {
                    if (item.Id == tipo1.ToString())
                    {
                        objTipo = item;
                    }
                }

            }
            Be.Best_categorias objCat = null;

            if (!string.IsNullOrEmpty(cat))
            {
                foreach (var item in menu)
                {

                    objCat = item.buscarCategoria(cat);

                    if (objCat.Nombre != null)
                    {
                        break;
                    }

                }

            }

